{
  "name": "crowd-health",
  "version": "0.0.1",
  "engines": {
    "node": "7.x"
  },
  "description": "CrowdHealth is a crowd-funded health and insurance platform.",
  "keywords": [
    "crowdfunding",
    "healthcare",
    "insurance",
    "crowd-funded"
  ],
  "author": [
    "Emily Engle <emily.engle.97@gmail.com>",
    "Josh Kuehn <kuehn.51@osu.edu>",
    "Kevin Unkrich <unkrich.5@osu.edu>",
    "Kyle Thompson <kyle@kylekthompson.com>"
  ],
  "license": "MIT",
  "scripts": {
    "build": "NODE_ENV=production webpack --config config/webpack.config.production.js",
    "lint": "esw . --ext .js,.jsx --color",
    "lint:watch": "npm run lint -- --watch",
    "test:client": "NODE_ENV=test karma start config/karma.conf.js",
    "test:client:watch": "npm run test:client -- --auto-watch --no-single-run",
    "test:server": "NODE_ENV=test mocha --recursive --colors spec/server",
    "test:server:watch": "npm run test:server -- --watch",
    "migrate:current": "knex --knexfile config/knexfile.js migrate:currentVersion",
    "migrate:latest": "knex --knexfile config/knexfile.js migrate:latest",
    "migrate:make": "node tools/createMigration",
    "migrate:rollback": "knex --knexfile config/knexfile.js migrate:rollback",
    "pretest": "tools/script/test && NODE_ENV=test npm run migrate:latest",
    "seed:make": "knex --knexfile config/knexfile.js seed:make --",
    "seed:run": "knex --knexfile config/knexfile.js seed:run",
    "server:dev": "NODE_ENV=development nodemon server --ignore client",
    "setup": "tools/script/setup",
    "start": "npm run server:dev",
    "test": "npm-run-all test:server test:client lint",
    "test:watch": "npm-run-all --parallel test:server:watch test:client:watch lint:watch"
  },
  "main": "server/index.jsx",
  "repository": {
    "type": "git",
    "url": "github.com/BuckeyeHackers/crowd-health"
  },
  "dependencies": {
    "autoprefixer": "^6.4.1",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.7",
    "babel-plugin-transform-react-constant-elements": "^6.9.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.9",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-latest": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "bcrypt": "^0.8.7",
    "body-parser": "^1.15.2",
    "chalk": "^1.1.3",
    "css-loader": "^0.25.0",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "history": "^4.3.0",
    "html-webpack-plugin": "^2.24.1",
    "http-proxy-middleware": "^0.17.1",
    "humps": "^2.0.0",
    "json-loader": "^0.5.4",
    "jsonwebtoken": "^7.1.9",
    "jwt-decode": "^2.1.0",
    "knex": "^0.12.4",
    "morgan": "^1.7.0",
    "node-sass": "^3.10.0",
    "pg": "^6.1.0",
    "pg-promise": "^5.3.3",
    "postcss-loader": "^1.1.0",
    "react": "^15.3.1",
    "react-dom": "^15.3.2",
    "react-hot-loader": "^3.0.0-beta.4",
    "react-redux": "^4.4.5",
    "react-router": "^4.0.0-alpha.5",
    "react-router-addons-controlled": "0.0.1",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.15.1",
    "webpack-md5-hash": "0.0.5",
    "whatwg-fetch": "^1.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.0.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "chai-http": "^3.0.0",
    "enzyme": "^2.4.1",
    "eslint": "^3.9.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.1",
    "eslint-plugin-react": "^6.2.0",
    "eslint-watch": "^2.1.14",
    "karma": "^1.3.0",
    "karma-mocha": "^1.2.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^1.8.0",
    "mocha": "^3.0.2",
    "nodemon": "^1.10.2",
    "npm-run-all": "^3.1.0",
    "phantomjs-polyfill": "0.0.2",
    "phantomjs-prebuilt": "^2.1.12",
    "react-addons-test-utils": "^15.3.2",
    "sinon": "^1.17.5"
  }
}
